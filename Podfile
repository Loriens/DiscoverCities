platform :ios, '12.0'

inhibit_all_warnings!

target 'RandomPlacePhoto' do
  use_frameworks!

  # Load images
  pod 'Kingfisher', '~> 5.7.0'

  # Network
  pod 'Alamofire', '~> 4.8.2'

  # Loader
  pod 'NVActivityIndicatorView', '~> 4.7.0'

  # Flickr
  pod 'FlickrKit', '1.0.9'

  # System messages
  pod 'Toast-Swift', '~> 5.0.0'

end
